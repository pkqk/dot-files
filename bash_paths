if [ -z "$DOT_PATH_SET" ]; then
  if [ -d "/usr/local/go/bin" ]
  then
    export PATH="/usr/local/go/bin:$PATH"
  fi

  export PATH=~/bin:$PATH
  export PATH="${PATH}:${HOME}/go/bin:${HOME}/go/bin/darwin_arm64:/opt/homebrew/bin:/opt/homebrew/opt/mysql-client@5.7/bin"
  export PATH="${PATH}:/opt/homebrew/opt/node@20/bin"
  export DOT_PATH_SET=yes
fi
